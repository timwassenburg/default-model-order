{
    "name": "timwassenburg/default-model-order",
    "description": "Adds a trait to make your models sortable",
    "keywords": ["php", "trait", "laravel", "eloquent", "models", "sortable"],
    "homepage": "https://github.com/timwassenburg/default-model-order",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Wassenburg"
        }
    ],
    "autoload": {
        "psr-4": {
            "TimWassenburg\\DefaultModelOrder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TimWassenburg\\DefaultModelOrder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TimWassenburg\\DefaultModelOrder\\DefaultModelOrderServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "nunomaduro/larastan": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
